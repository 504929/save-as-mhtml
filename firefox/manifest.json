{
  "manifest_version": 2,
  "version": "0.1.5",
  "name": "Save as MHTML",
  "description": "Edit and save active tab in MHTL/MHT (MIME HTML) archive file format",
  "background": {
    "persistent": false,
    "scripts": [
      "pageCapture.js",
      "background.js"
  ]},
  "permissions": [
    "storage",
    "pageCapture",
    "activeTab",
    "downloads",
    "notifications",
    "*://*/*"
  ],
  "homepage_url": "https://add0n.com/save-as-mhtml.html",
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "browser_action": {},
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "web_accessible_resources": [
    "data/toolbar/index.html"
  ]
}
